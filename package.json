{
  "name": "eventio",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "pretty-quick": "pretty-quick --staged",
    "lint": "tslint -p ./src",
    "clean:babel": "rm -rf ./node_modules/.cache/babel-loader",
    "clean": "yarn clean:babel",
    "dev": "yarn clean && node server.js",
    "heroku-postbuild": "yarn build",
    "now-build": "echo 'Built on Buddy CI'",
    "build": "yarn clean && next build src && cp -r src/static ./.next",
    "now:deploy": "now --token ${NOW_TOKEN} --local-config=now/${NOW_ENV}.json",
    "now:alias": "now alias --token ${NOW_TOKEN} --local-config=now/${NOW_ENV}.json",
    "deploy": "yarn now:deploy && yarn now:alias",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "analyze:server": "BUNDLE_ANALYZE=server yarn build",
    "analyze:browser": "BUNDLE_ANALYZE=browser yarn build",
    "start": "yarn clean && NODE_ENV=production node server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty-quick"
    }
  },
  "dependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-typescript": "1.1.1",
    "axios": "^0.18.0",
    "compression": "1.7.3",
    "cookie": "^0.3.1",
    "dayjs": "^1.8.8",
    "express": "4.16.4",
    "fontfaceobserver": "^2.1.0",
    "fork-ts-checker-webpack-plugin": "0.5.2",
    "immer": "^2.1.1",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^2.2.0",
    "next": "8.0.3",
    "next-compose-plugins": "2.1.1",
    "next-offline": "3.3.6",
    "next-progressbar": "1.0.0",
    "next-redux-wrapper": "3.0.0-alpha.2",
    "next-routes": "1.4.2",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-redux": "^6.0.1",
    "redux": "4.0.1",
    "redux-act": "1.7.4",
    "redux-devtools-extension": "2.13.8",
    "redux-saga": "0.16.0",
    "reselect": "^4.0.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@types/axios": "^0.14.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/next": "8.0.1",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "@types/react-redux": "7.0.1",
    "@types/styled-components": "^4.1.11",
    "@types/webpack-env": "^1.13.8",
    "babel-plugin-inline-dotenv": "1.2.1",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "husky": "1.3.1",
    "next-plugin-transpile-modules": "2.0.0",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "tslib": "1.9.3",
    "tslint": "5.13.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3333"
  },
  "resolutions": {
    "next-page-transitions/react-transition-group": "2.5.1"
  },
  "peerDependencies": {}
}
